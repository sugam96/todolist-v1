<%- include('header'); -%>
<div class="box" id="heading">
    <h1>
        <%= listTitle %>
    </h1>
</div>
<div class="box">
    <% items.forEach(item=> { %>
        <div class="item">
            <input type="checkbox" name="" id="">
            <p>
                <%= item %>
            </p>
        </div>
        <% }) %>
            <form action="/" method="post">
                <input type="text" name="newItem" placeholder="New Task" autocomplete="off">
                <button type="submit" value="<%= listTitle %>" name="list">+</button>
            </form>
</div>
<%- include('footer'); -%>